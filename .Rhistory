# Create `peak_timing` by identifying the day of peak adult abundance (A) per replicate
peak_timing <- results_replicates %>%
group_by(Scenario, Replicate) %>%
slice_max(order_by = A, n = 1, with_ties = FALSE) %>%
ungroup() %>%
select(Scenario, Replicate, Day_of_Year = time)  # Rename `time` as day-of-year
# 1. Compute mean peak timing per scenario
peak_means <- peak_timing %>%
group_by(Scenario) %>%
summarise(Mean_Peak_Day = mean(Day_of_Year), .groups = "drop")
# 2. Plot violin + boxplot + mean
plot_violin <- ggplot(peak_timing, aes(x = Scenario, y = Day_of_Year, fill = Scenario)) +
geom_violin(trim = FALSE, alpha = 0.6, color = "black") +
geom_boxplot(width = 0.1, outlier.shape = NA, fill = "white") +
geom_point(data = peak_means, aes(x = Scenario, y = Mean_Peak_Day),
shape = 16, size = 3, color = "black", fill = "white", stroke = 1.2) +
scale_y_continuous(breaks = c(15, 45, 75, 105, 135, 165, 195, 225, 255, 285, 315, 345),
labels = c("Jan", "Feb", "Mar", "Apr", "May", "Jun",
"Jul", "Aug", "Sep", "Oct", "Nov", "Dec")) +
scale_fill_manual(values = c("baseline" = "grey", "dry" = "blue",
"wet" = "red", "erratic" = "yellow")) +
labs(
title = "Timing of Peak Adult Abundance by Rainfall Scenario (Years 3–5)",
x = "Rainfall Scenario",
y = "Month of Peak Abundance"
) +
theme_bw() +
theme(
strip.text = element_text(size = 12, face = "bold"),
legend.position = "none",
plot.title = element_text(face = "bold", size = 14),
axis.title = element_text(size = 12, face = "bold"),
axis.text.x = element_text(size = 12),
axis.text.y = element_text(size = 10)
)
plot_violin
